{
    "rust": {
      "command": "rust-analyzer",
      "filetypes": ["rust"],
      "rootPatterns": ["Cargo.toml"]
    },
    "reason": {
      "command": "reason-language-server",
      "filetypes": ["reason"]
    },
    "eslint": {
        "run": "onSave",
        "autoFixOnSave": true
    },
    "svelte.plugin.typescript.enable": false,
    "prettier.onlyUseLocalVersion": false,
    "coc.preferences.formatOnSaveFiletypes": ["css", "javascript", "svelte", "javascriptreact", "markdown"]
}
